<manifest package="com.vol.pdvlib"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity
            android:name=".DrawableListActivity"
            android:label="@string/drawables_list"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".DrawableDetailActivity"
            android:label="@string/title_drawable_detail"
            android:parentActivityName=".DrawableListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vol.pdvlib.DrawableListActivity"/>
        </activity>

    </application>

</manifest>
